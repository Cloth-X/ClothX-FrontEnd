    "@ngxs/logger-plugin": "^3.2.0",
    "@ngxs/router-plugin": "^3.2.0",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "@zxing/ngx-scanner": "^1.2.1",
    "angularfire2": "^5.1.0",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.3",
